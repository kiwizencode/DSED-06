@model IEnumerable<DSED06_WebApp.Models.AQUATIC_GROUP>

@{
    ViewBag.Title = "Index";
}

<h2>Aquatic Grouping Maintenance Page</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @* @Html.DisplayNameFor(model => model.NAME)*@
            Grouping Name
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.OrderBy(x=>x.NAME)) {
        <tr>
            <td>
               @Html.DisplayFor(modelItem => item.NAME)        
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.ID_PK }) |
                @Html.ActionLink("Details", "Details", new { id=item.ID_PK }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.ID_PK })
            </td>
        </tr>
}

</table>

<div>
    @Html.ActionLink("Back to Main Page", "Index", "Home")
</div>

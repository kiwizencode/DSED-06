@model DSED06_Aquatic_Pet_Store.Models.PET_INFO

@{
    ViewBag.Title = "Delete";
}

<h2>Delete Pet Information</h2>

<h3>Are you sure you want to delete this?</h3>
<div>
    <!-- <h4>AQUATIC_PET</h4>  -->
    
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @* @Html.DisplayNameFor(model => model.COMMON)  *@
            Common Name :
        </dt>

        <dd>
            @Html.DisplayFor(model => model.COMMON)
        </dd>

        <dt>
            @* @Html.DisplayNameFor(model => model.SCIENTIFIC)  *@
            Scientific Name :
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SCIENTIFIC)
        </dd>

        <dt>
            @* @Html.DisplayNameFor(model => model.AQUATIC_GROUP.NAME)  *@
            Group :
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PET_GROUP.NAME)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default  col-md-offset-1" />
            <!--  |  -->
            <p>&nbsp</p>
            @Html.ActionLink("Back to Pet Information Maintenance Page", "Index")
        </div>
    }
</div>
